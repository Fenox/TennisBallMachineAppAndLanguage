package bmsp_unit_test;

import java.util.ArrayList;

import org.junit.Test;

import bmsp.commands.Command;
import bmsp.commands.ShootCommand;
import bmsp.commands.StartCommand;

public class BMSPWriteReadTest {

	@Test
	public void testWriteAndRead() {
		
		ArrayList<Command> cmds = generateCommands();
		
	}
	
	private ArrayList<Command> generateCommands() {
		ArrayList<Command> cmds = new ArrayList<>();
		cmds.add(new StartCommand());
		
		ShootCommand cmd1 = new ShootCommand();
		cmd1.setHeigth(10);
		cmd1.setSpeed(100);
		cmd1.setSpin(2000);
		cmd1.setTime(2000);
		cmd1.setX(5);
		cmd1.setY(10);
	}
}
